{
    "name": "VK AutoFriends",
    "author": "skjultforsolen",
    "description": "Add automatically friends",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["reload.js"]
        }
      ],
    "version": "2.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["scripting", "activeTab", "storage"],
    "background": {
        "service_worker": "background.js"
      }
      
}