{
    "name": "VK Script",
    "description": "VK Script",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["reload.js"]
        }
      ],
    "version": "1.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["scripting", "activeTab", "storage"],
    "background": {
        "service_worker": "background.js"
      }
      
}